{
    "name": "cosmow/notification-bundle",
    "description": "Cosmomedia's Notification Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Milayton Pereira",
            "email": "milayton.pereira@cosmomedia.es"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/doctrine-bundle": "^3.0",
        "doctrine/persistence": "^2.0 || ^3.3.3",
        "sonata-project/doctrine-extensions": "^1.10.1",
        "symfony/config": "^6.0",
        "symfony/console": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/event-dispatcher": "^6.0",
        "symfony/form": "^6.0",
        "symfony/http-foundation": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/security-core": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Cosmow\\NotificationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cosmow\\NotificationBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
